{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "outDir": "./dist",
    "strict": true,
    "baseUrl": "./",
    "typeRoots": [
      "node_modules/@types"
    ],
    "types": [
      "node", "@types/jest"
    ],
    "esModuleInterop": true,
    "inlineSourceMap": true,
    "strictPropertyInitialization" : false,
    "resolveJsonModule": true
  },
  "include": ["src"]
}
