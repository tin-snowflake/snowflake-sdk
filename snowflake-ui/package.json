{
  "name": "snowflake-ui",
  "version": "0.1.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/jtomtran/snowflake.git"
  },
  "homepage": ".",
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "program:build": "cd program && cargo build-bpf",
    "program:test": "cd program && cargo test-bpf",
    "localnet:up": "solana-test-validator --bpf-program BpfProgram1111111111111111111111111111111111 program/target/deploy/bpf_program_template.so --reset",
    "localnet:logs": "solana logs -u localhost",
    "predeploy": "git pull --ff-only && yarn && yarn build",
    "deploy": "aws s3 sync build/ s3://app.snowflake.so",
    "deploy:ar": "arweave deploy-dir build --key-file ",
    "format:fix": "prettier --write \"**/*.+(js|jsx|ts|tsx|json|css|md)\""
  },
  "dependencies": {
    "@orca-so/sdk": "^1.2.7",
    "decimal.js": "^10.3.1",
    "@solana/spl-token-swap": "^0.1.0",
    "@ant-design/icons": "^4.5.0",
    "@craco/craco": "^5.7.0",
    "@dbricks/dbricks-ts": "0.0.67",
    "@project-serum/anchor": "^0.16.2",
    "@project-serum/serum": "^0.13.34",
    "@project-serum/swap": "^0.1.0-alpha.32",
    "@quarryprotocol/quarry-sdk": "^1.8.2",
    "@saberhq/stableswap-sdk": "^1.4.1",
    "@solana/spl-token": "^0.1.6",
    "@solana/spl-token-registry": "^0.2.285",
    "@solana/wallet-adapter-ant-design": "^0.1.0",
    "@solana/wallet-adapter-base": "^0.4.1",
    "@solana/wallet-adapter-react": "^0.6.0",
    "@solana/wallet-adapter-wallets": "^0.6.0",
    "@solana/web3.js": "^1.22.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@types/react-router-dom": "^5.1.6",
    "antd": "^4.6.6",
    "aws": "0.0.3-2",
    "bn.js": "^5.1.3",
    "bs58": "^4.0.1",
    "buffer-layout": "^1.2.0",
    "craco-less": "^1.17.0",
    "cron-converter": "^1.0.1",
    "deep-equal": "^2.0.5",
    "eventemitter3": "^4.0.7",
    "jazzicon": "^1.5.0",
    "later": "^1.2.0",
    "lodash": "^4.17.21",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-ellipsis-text": "^1.2.1",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.3.1",
    "react-loader-spinner": "^4.0.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "typescript": "^4.4.2"
  },
  "devDependencies": {
    "@types/bn.js": "^4.11.6",
    "@types/bs58": "^4.0.1",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.20.0",
    "@types/react": "^16.9.50",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "arweave-deploy": "^1.9.1",
    "gh-pages": "^3.1.0",
    "prettier": "^2.1.2",
    "tslint": "^6.1.3",
    "usehooks-ts": "^1.2.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "/node_modules/(?!@project-serum/sol-wallet-adapter)"
    ]
  }
}
